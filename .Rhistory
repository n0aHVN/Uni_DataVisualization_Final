library(tidyverse)
library(knitr)
library(caret)
library(dplyr)
DSThanhVien <-data.frame("STT" = c(9,10,30),
"Name" = c("Nguyễn Trí Dũng","Lê Minh Đăng","Nguyễn Khoa Quang Thắng"),
